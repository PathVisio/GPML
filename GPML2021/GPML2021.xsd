<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://pathvisio.org/GPML/2021" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gpml="http://pathvisio.org/GPML/2021" elementFormDefault="qualified">
	<!-- Graphical Pathway Markup Language version 2021 -->
	<!-- 
	     Type Definitions 
         This block declares properties which can be attached to elements in the GPML hierarchy:
		 - attributeGroups: RectAttributes, FontAttributes, ShapeStyleAttributes, LineStyleAttributes
		 - simpleTypes: StyleType, Dimension
		 - element group: CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef) 
    -->
	<!-- 
		RectProperties, Centering (Position) and Dimension Properties, in the order to be written. 
	-->
	<xsd:attributeGroup name="RectAttributes">
		<xsd:annotation>
			<xsd:documentation>Centering (position) and dimension properties for an object.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="centerX" type="xsd:float" use="required">
			<xsd:annotation>
				<xsd:documentation>The middle of an object in the x-direction.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="centerY" type="xsd:float" use="required">
			<xsd:annotation>
				<xsd:documentation>The middle of an object in the y-direction.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="width" type="gpml:Dimension" use="required">
			<xsd:annotation>
				<xsd:documentation>The pixel value for the x-dimensional length of an object.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="height" type="gpml:Dimension" use="required">
			<xsd:annotation>
				<xsd:documentation>The pixel value for the y-dimensional length of an object.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!-- 
		Font Properties in the order to be written. 
	-->
	<xsd:attributeGroup name="FontAttributes">
		<xsd:annotation>
			<xsd:documentation>Common font properties.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="textColor" type="xsd:hexBinary" default="000000">
			<xsd:annotation>
				<xsd:documentation>The hexadecimal color of text, default black.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fontName" default="Arial" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>The name of the collection of characters with a similar design, e.g. Arial (default)...</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fontWeight" default="Normal">
			<xsd:annotation>
				<xsd:documentation>The thickness of the font used, e.g Normal (default) or Bold. A bold font has more weight.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Normal"/>
					<xsd:enumeration value="Bold"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="fontStyle" default="Normal">
			<xsd:annotation>
				<xsd:documentation>The typographic style for Italic or Normal (default).</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Normal"/>
					<xsd:enumeration value="Italic"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="fontDecoration" default="Normal">
			<xsd:annotation>
				<xsd:documentation>The typographic style for Underline or Normal (default).</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Normal"/>
					<xsd:enumeration value="Underline"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="fontStrikethru" default="Normal">
			<xsd:annotation>
				<xsd:documentation>The typographic style for Strikethru or Normal (default).</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Normal"/>
					<xsd:enumeration value="Strikethru"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="fontSize" type="xsd:nonNegativeInteger" default="12">
			<xsd:annotation>
				<xsd:documentation>The point value for the size of the font.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="hAlign" default="Center">
			<xsd:annotation>
				<xsd:documentation>The horizontal alignment of displayed text: Left, Center (default), or Right.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Left"/>
					<xsd:enumeration value="Center"/>
					<xsd:enumeration value="Right"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="vAlign" default="Middle">
			<xsd:annotation>
				<xsd:documentation>The vertical alignment of displayed text: Top, Middle (default), or Bottom.</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Top"/>
					<xsd:enumeration value="Middle"/>
					<xsd:enumeration value="Bottom"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!-- 
		Shape Style Properties in the order to be written.  
	-->
	<xsd:attributeGroup name="ShapeStyleAttributes">
		<xsd:annotation>
			<xsd:documentation>Visual appearance of a two-dimensional object.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="borderColor" type="xsd:hexBinary" default="000000">
			<xsd:annotation>
				<xsd:documentation>The hexadecimal color of a border, default black.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="borderStyle" type="gpml:StyleType" default="Solid">
			<xsd:annotation>
				<xsd:documentation>The visual appearance of a line, e.g. Solid (default), Dashed...</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="borderWidth" type="xsd:float" default="1.0">
			<xsd:annotation>
				<xsd:documentation>The pixel value for the width of a given border.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fillColor" type="xsd:hexBinary" default="ffffff">
			<xsd:annotation>
				<xsd:documentation>The hexadecimal color contained within the line or border of an object, default white.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="shapeType" type="xsd:string" default="Rectangle">
			<xsd:annotation>
				<xsd:documentation>The visual appearance of a two dimensional object, e.g. Rectangle (default), Nucleus...</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="zOrder" type="xsd:integer" use="optional">
			<xsd:annotation>
				<xsd:documentation>The order of an object along the z-axis.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!-- 
		Line Style Properties in the order to be written.  
	-->
	<xsd:attributeGroup name="LineStyleAttributes">
		<xsd:annotation>
			<xsd:documentation>Visual appearance of a line.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="lineColor" type="xsd:hexBinary" default="000000">
			<xsd:annotation>
				<xsd:documentation>The hexadecimal color of a line, default black.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="lineStyle" type="gpml:StyleType" default="Solid">
			<xsd:annotation>
				<xsd:documentation>The visual appearance of a line, e.g. Solid (default), Dashed...</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="lineWidth" type="xsd:float" default="1.0">
			<xsd:annotation>
				<xsd:documentation>The pixel value for the width of a given line.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="connectorType" default="Straight">
			<xsd:annotation>
				<xsd:documentation>Specifies a set of rules to govern layout of lines, e.g. Straight (default), Elbow...</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:union>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="Straight"/>
							<xsd:enumeration value="Elbow"/>
							<xsd:enumeration value="Curved"/>
							<xsd:enumeration value="Segmented"/>
						</xsd:restriction>
					</xsd:simpleType>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string"/>
					</xsd:simpleType>
				</xsd:union>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="zOrder" type="xsd:integer" use="optional">
			<xsd:annotation>
				<xsd:documentation>The order of an object along the z-axis.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!-- 
		Style Simple Type for Lines or Borders 
	-->
	<xsd:simpleType name="StyleType">
		<xsd:annotation>
			<xsd:documentation>Style type for a line or border, e.g. Solid, Dashed...</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Solid"/>
					<xsd:enumeration value="Dashed"/>
					<xsd:enumeration value="Double"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<!-- 
		Simple Type for Dimensions 
	-->
	<xsd:simpleType name="Dimension">
		<xsd:annotation>
			<xsd:documentation>Dimension for width or height. Cannot be a negative value.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:float">
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- 
		Comment Group in the order to be written. 
	-->
	<xsd:group name="CommentGroup">
		<xsd:annotation>
			<xsd:documentation>One or more comments, properties, or references.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Comment" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Descriptions or arbitrary notes for an object.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:string">
							<xsd:attribute name="source" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>The source for a comment.</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Property" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Key-value pair information for an object.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="key" type="xsd:string" use="required">
						<xsd:annotation>
							<xsd:documentation>The key of a key-value pair.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="value" type="xsd:string" use="required">
						<xsd:annotation>
							<xsd:documentation>The value of a key-value pair.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="gpml:AnnotationRef" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Reference to a gpml:Annotation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="gpml:CitationRef" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Reference to a gpml:Citation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="gpml:EvidenceRef" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Reference to a gpml:Evidence.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- End: Type Definitions -->
	<!-- ###################################################################### -->
	<!-- 
		Pathway 
        This is the root element of the structure. 
		
		Pathway properties in the order to be written in the gpml:
		- title, organism, source, version, license
		- Xref, Description, Authors
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef)
		- Graphics (boardWidth, boardHeight, backgroundColor), InfoBox
		- DataNodes, Interactions, GraphicalLines, Labels, Shapes, Groups
		- Annotations, Citations, Evidences
	-->
	<xsd:element name="Pathway">
		<xsd:annotation>
			<xsd:documentation>A graph diagram representing a biological process as a set of interactions and relationships among genes, proteins, metabolites, and other factors in the context of cellular compartments, tissues and organisms.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="gpml:Xref" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="Description" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Textual description of the pathway.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string"/>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Authors" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Author" minOccurs="0" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>The people who created or edited a pathway.</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element ref="gpml:Xref" minOccurs="0" maxOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Reference to a database entry for an author, e.g. ORCID.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
									<xsd:attribute name="name" type="xsd:string" use="required">
										<xsd:annotation>
											<xsd:documentation>The name of an author.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="username" type="xsd:string" use="optional">
										<xsd:annotation>
											<xsd:documentation>The username of an author.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="order" type="xsd:integer" use="optional">
										<xsd:annotation>
											<xsd:documentation>The authorship order of an author.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:group ref="gpml:CommentGroup"/>
				<!-- Graphics related attributes-->
				<xsd:element name="Graphics" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="boardWidth" type="gpml:Dimension" use="required">
							<xsd:annotation>
								<xsd:documentation>The width of a pathway.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="boardHeight" type="gpml:Dimension" use="required">
							<xsd:annotation>
								<xsd:documentation>The height of a pathway.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="backgroundColor" type="xsd:hexBinary" default="ffffff">
							<xsd:annotation>
								<xsd:documentation>The hexadecimal color for the background of a pathway, default white.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End Graphics related attributes-->
				<xsd:element ref="gpml:InfoBox" minOccurs="1" maxOccurs="1"/>
				<xsd:element ref="gpml:DataNodes" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:Interactions" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:GraphicalLines" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:Labels" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:Shapes" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:Groups" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:Annotations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:Citations" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="gpml:Evidences" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="title" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation>The title of a pathway.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="organism" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation>The scientific name of the species being described by a pathway, e.g. Homo sapiens...</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="source" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation>The source of a pathway, e.g. WikiPathways, KEGG...</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="version" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation>The version of a pathway.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="license" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation>The license of a pathway.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Pathway -->
	<!-- ###################################################################### -->
	<!-- 
		Data Nodes 
		The elements with biological context forming the basis of the biological pathway.
		DataNode represents all possible biological entities in a pathway like GeneProduct, 
		Metabolite, Pathway, Protein, RNA...
		
		DataNode properties in the order to be written: 
		- elementId, textLabel, type, groupRef, elementRef 
		- Xref, States
		- Graphics (RectAttributes, FontAttributes, ShapeStyleAttributes)
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef) 
	-->
	<xsd:element name="DataNodes">
		<xsd:annotation>
			<xsd:documentation>List of Data nodes. A data node denotes a biological entity that forms a node in a pathway.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- DataNode -->
				<xsd:element name="DataNode" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Denotes a biological entity that forms a node in a pathway.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="gpml:Xref" minOccurs="0" maxOccurs="1"/>
							<xsd:element ref="gpml:States" minOccurs="0" maxOccurs="1">
								<xsd:annotation>
									<xsd:documentation>The states linked to this data node.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<!-- Graphics related attributes-->
							<xsd:element name="Graphics" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attributeGroup ref="gpml:RectAttributes"/>
									<xsd:attributeGroup ref="gpml:FontAttributes"/>
									<xsd:attributeGroup ref="gpml:ShapeStyleAttributes"/>
								</xsd:complexType>
							</xsd:element>
							<!-- End Graphics related attributes-->
							<xsd:group ref="gpml:CommentGroup"/>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="textLabel" type="xsd:string" use="required">
							<xsd:annotation>
								<xsd:documentation>The text label for a data node.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="type" default="Undefined">
							<xsd:annotation>
								<xsd:documentation>The data node type, e.g. Undefined (default), GeneProduct, Metabolite, Alias...</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:union>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string">
											<xsd:enumeration value="Undefined"/>
											<xsd:enumeration value="GeneProduct"/>
											<xsd:enumeration value="DNA"/>
											<xsd:enumeration value="RNA"/>
											<xsd:enumeration value="Protein"/>
											<xsd:enumeration value="Complex"/>
											<xsd:enumeration value="Metabolite"/>
											<xsd:enumeration value="Pathway"/>
											<xsd:enumeration value="Disease"/>
											<xsd:enumeration value="Phenotype"/>
											<xsd:enumeration value="Alias"/>
											<xsd:enumeration value="Event"/>
										</xsd:restriction>
									</xsd:simpleType>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string"/>
									</xsd:simpleType>
								</xsd:union>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="groupRef" type="xsd:IDREF" use="optional">
							<xsd:annotation>
								<xsd:documentation>When a pathway element is part of a gpml:Group, groupRef refers to the elementId of the parent group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="elementRef" type="xsd:IDREF" use="optional">
							<xsd:annotation>
								<xsd:documentation>An elementRef indicates a child/parent relationship between objects. The elementRef of the child refers to the elementId of the parent. A DataNode can have type "Alias" and refer to a gpml:Group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: DataNode -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: DataNodes -->
	<!-- ###################################################################### -->
	<!-- 
		States         
		Always linked to a DataNode representing a specific state of the biological 
		entity (e.g. phosphorylation, genetic variants, etc)

		State properties in the order to be written: 
		- elementId, textLabel, type
		- Xref
		- Graphics (relX, relY,  width, height, FontAttributes, ShapeStyleAttributes)
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef)
	-->
	<xsd:element name="States">
		<xsd:annotation>
			<xsd:documentation>List of States. A state represents a specific state of the biological entity (e.g. phosphorylation, genetic variants, etc). A state is linked to a data node.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- State -->
				<xsd:element name="State" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>The state of a biological entity (e.g. Undefined (default), Protein modification...) linked to a parent gpml:DataNode.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="gpml:Xref" minOccurs="0" maxOccurs="1"/>
							<!-- Graphics related attributes-->
							<xsd:element name="Graphics" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attribute name="relX" type="xsd:float" use="required">
										<xsd:annotation>
											<xsd:documentation>The relative x coordinates on the parent gpml:DataNode, where 0,0 is at the center and 1,1 is at the bottom-right corner of the data node.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="relY" type="xsd:float" use="required">
										<xsd:annotation>
											<xsd:documentation>The relative y coordinates on the parent gpml:DataNode, where 0,0 is at the center and 1,1 is at the bottom-right corner of the data node.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="width" type="gpml:Dimension" use="required">
										<xsd:annotation>
											<xsd:documentation>The pixel value for the x-dimensional length of a state.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="height" type="gpml:Dimension" use="required">
										<xsd:annotation>
											<xsd:documentation>The pixel value for the y-dimensional length of a state.</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attributeGroup ref="gpml:FontAttributes"/>
									<xsd:attributeGroup ref="gpml:ShapeStyleAttributes"/>
								</xsd:complexType>
							</xsd:element>
							<!-- End Graphics related attributes-->
							<xsd:group ref="gpml:CommentGroup"/>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="textLabel" use="required" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>The text label for a state.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="type" default="Undefined">
							<xsd:annotation>
								<xsd:documentation>The state type, e.g. Undefined (default), Protein modification, Genetic variant...</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:union>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string">
											<xsd:enumeration value="Undefined"/>
											<xsd:enumeration value="ProteinModification"/>
											<xsd:enumeration value="GeneticVariant"/>
											<xsd:enumeration value="EpigeneticModification"/>
										</xsd:restriction>
									</xsd:simpleType>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string"/>
									</xsd:simpleType>
								</xsd:union>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: State -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: States -->
	<!-- ###################################################################### -->
	<!-- 
		Interactions
		Represent biological relation between entities. Interaction and GraphicalLines together 
		are the only elements used for implementing any kind of connections or interactions.
		
		Interaction properties in the order to be written: 
		- elementId, groupRef
		- Xref, Waypoints (Point, Anchor)
		- Graphics (LineStyleAttributes)
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef)
	-->
	<xsd:element name="Interactions">
		<xsd:annotation>
			<xsd:documentation>List of Interactions. An interaction represents the biological relation between entities. An Xref can be specified for Interactions (in contrast to GraphicalLines).</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- Interaction -->
				<xsd:element name="Interaction" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Interactions represent biological relation between entities. An Xref can be specified for Interactions (in contrast to GraphicalLines).</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="gpml:Xref" minOccurs="0" maxOccurs="1"/>
							<xsd:element name="Waypoints" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element ref="gpml:Point" minOccurs="2" maxOccurs="unbounded"/>
										<xsd:element ref="gpml:Anchor" minOccurs="0" maxOccurs="unbounded"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Graphics related attributes-->
							<xsd:element name="Graphics" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attributeGroup ref="gpml:LineStyleAttributes"/>
								</xsd:complexType>
							</xsd:element>
							<!-- End Graphics related attributes-->
							<xsd:group ref="gpml:CommentGroup"/>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="groupRef" type="xsd:IDREF" use="optional">
							<xsd:annotation>
								<xsd:documentation>When a pathway element is part of a gpml:Group, groupRef refers to the elementId of the parent group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: Interaction -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Interactions -->
	<!-- ###################################################################### -->
	<!-- 
		GraphicalLines
		Graphical annotations without semantic meaning. Interaction and GraphicalLines together 
		are the only elements used for implementing any kind of connections or interactions.
		
		GraphicalLine properties in the order to be written: 
		- elementId, groupRef
		- Waypoints (Point, Anchor)
		- Graphics (LineStyleAttributes)
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef)
    -->
	<xsd:element name="GraphicalLines">
		<xsd:annotation>
			<xsd:documentation>List of Graphical lines. A graphical line is a visual annotation without semantic meaning. It is often used to partition space or connect other pathway elements. An Xref cannot be specified for GraphicalLines (in contrast to Interactions).</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- GraphicalLine -->
				<xsd:element name="GraphicalLine" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Graphical line are visual annotations without semantic meaning. An Xref cannot be specified for GraphicalLines (in contrast to Interactions).</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Waypoints" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element ref="gpml:Point" minOccurs="2" maxOccurs="unbounded"/>
										<xsd:element ref="gpml:Anchor" minOccurs="0" maxOccurs="unbounded"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Graphics related attributes-->
							<xsd:element name="Graphics" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attributeGroup ref="gpml:LineStyleAttributes"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:group ref="gpml:CommentGroup"/>
							<!-- End Graphics related attributes-->
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="groupRef" type="xsd:IDREF" use="optional">
							<xsd:annotation>
								<xsd:documentation>When a pathway element is part of a gpml:Group, groupRef refers to the elementId of the parent group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: GraphicalLine -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: GraphicalLines -->
	<!-- ###################################################################### -->
	<!-- 
		Labels 
		An element to attach simple labels to the pathway
		No semantic meaning - graphical attributes
		
		Label properties in the order to be written: 
		- elementId, textLabel, groupRef, href
		- Graphics (RectAttributes, FontAttributes, ShapeStyleAttributes)
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef)
	-->
	<xsd:element name="Labels">
		<xsd:annotation>
			<xsd:documentation>List of Labels. A label is a text field which can be used to annotate any aspect of a pathway.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- Label -->
				<xsd:element name="Label" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>A text field which can be used to annotate any aspect of a pathway.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<!-- Graphics related attributes-->
							<xsd:element name="Graphics" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attributeGroup ref="gpml:RectAttributes"/>
									<xsd:attributeGroup ref="gpml:FontAttributes"/>
									<xsd:attributeGroup ref="gpml:ShapeStyleAttributes"/>
								</xsd:complexType>
							</xsd:element>
							<!-- End Graphics related attributes-->
							<xsd:group ref="gpml:CommentGroup"/>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="textLabel" use="required" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>The text for a label.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="groupRef" type="xsd:IDREF" use="optional">
							<xsd:annotation>
								<xsd:documentation>When a pathway element is part of a gpml:Group, groupRef refers to the elementId of the parent group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="href" use="optional" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>The hyperlink for a reference to an url.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: Label -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Labels -->
	<!-- ###################################################################### -->
	<!-- 
		Shapes 
	    Graphical elements - shapes with or without a text label
		
		Shape properties in the order to be written: 
		- elementId, textLabel, groupRef
		- Graphics (RectAttributes, FontAttributes, ShapeStyleAttributes), rotation
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef)
	-->
	<xsd:element name="Shapes">
		<xsd:annotation>
			<xsd:documentation>List of Shapes. A shape is a visual annotation or graphical element.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- Shapes -->
				<xsd:element name="Shape" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>A visual annotation or graphical element.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<!-- Graphics related attributes-->
							<xsd:element name="Graphics" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attributeGroup ref="gpml:RectAttributes"/>
									<xsd:attributeGroup ref="gpml:FontAttributes"/>
									<xsd:attributeGroup ref="gpml:ShapeStyleAttributes"/>
									<xsd:attribute name="rotation" type="xsd:float" default="0.0">
										<xsd:annotation>
											<xsd:documentation>A float value (rotation in radians).</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
							<xsd:group ref="gpml:CommentGroup"/>
							<!-- End Graphics related attributes-->
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="textLabel" use="optional" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>The text label for a shape.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="groupRef" type="xsd:IDREF" use="optional">
							<xsd:annotation>
								<xsd:documentation>When a pathway element is part of a gpml:Group, groupRef refers to the elementId of the parent group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: Shape -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Shapes -->
	<!-- ###################################################################### -->
	<!-- 
		Groups 
		Pathway Elements (DataNode, Interaction, GraphicalLine, Label, Shape, and Group) in the pathway can be grouped. 
		
		Group properties in the order to be written: 
		- elementId, textLabel, type, groupRef
		- Xref
		- Graphics (RectAttributes, FontAttributes, ShapeStyleAttributes)
		- CommentGroup (Comment, Property, AnnotationRef, CitationRef, EvidenceRef)
	-->
	<xsd:element name="Groups">
		<xsd:annotation>
			<xsd:documentation>List of Groups. A group is a collection of structurally or functionally similar or related pathway elements.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- Group -->
				<xsd:element name="Group" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>A collection of structurally or functionally similar or related pathway elements.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="gpml:Xref" minOccurs="0" maxOccurs="1"/>
							<!-- Graphics related attributes-->
							<xsd:element name="Graphics" minOccurs="1" maxOccurs="1">
								<xsd:complexType>
									<xsd:attributeGroup ref="gpml:RectAttributes"/>
									<xsd:attributeGroup ref="gpml:FontAttributes"/>
									<xsd:attributeGroup ref="gpml:ShapeStyleAttributes"/>
								</xsd:complexType>
							</xsd:element>
							<!-- End Graphics related attributes-->
							<xsd:group ref="gpml:CommentGroup"/>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="textLabel" type="xsd:string" use="optional">
							<xsd:annotation>
								<xsd:documentation>The text label for a group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="type" default="Group">
							<xsd:annotation>
								<xsd:documentation>The group type, e.g. Group (default), Complex, Pathway...</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:union>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string">
											<xsd:enumeration value="Group"/>
											<xsd:enumeration value="Complex"/>
											<xsd:enumeration value="Pathway"/>
											<xsd:enumeration value="Analog"/>
											<xsd:enumeration value="Paralog"/>
										</xsd:restriction>
									</xsd:simpleType>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string"/>
									</xsd:simpleType>
								</xsd:union>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="groupRef" type="xsd:IDREF" use="optional">
							<xsd:annotation>
								<xsd:documentation>When a pathway element is part of a gpml:Group, groupRef refers to the elementId of the parent group.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: Group -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Groups -->
	<!-- ###################################################################### -->
	<!-- 
		InfoBox 
		Holds the coordinates for where meta-information (e.g. title, organism) are displayed in the pathway. 
		
		InfoBox properties in the order to be written: 
		- centerX, centerY
	-->
	<xsd:element name="InfoBox">
		<xsd:annotation>
			<xsd:documentation>The information box holds the coordinates for where information (e.g. title and organism) are displayed in the pathway.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="centerX" type="xsd:float" use="required">
				<xsd:annotation>
					<xsd:documentation>The middle of the infobox in the x-direction.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="centerY" type="xsd:float" use="required">
				<xsd:annotation>
					<xsd:documentation>The middle of the infobox in the x-direction.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: InfoBox -->
	<!-- ###################################################################### -->
	<!-- 
		Annotations
		Pathway or pathway element AnnotationRefs refer to Annotations. 
		
		Annotation properties in the order to be written: 
		- elementId, value, type 
		- Xref, Url
	-->
	<xsd:element name="Annotations">
		<xsd:annotation>
			<xsd:documentation>List of Annotations. An annotation is a reference with additional information, e.g. some Ontology.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- Annotation -->
				<xsd:element name="Annotation" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>An annotation reference, e.g. Ontology (e.g.subcellular location), Taxonomy (e.g.organism)...</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="gpml:Xref" minOccurs="0" maxOccurs="1">
								<xsd:annotation>
									<xsd:documentation>Reference to a database entry. An Annotation can have an Xref.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element ref="gpml:Url" minOccurs="0" maxOccurs="1">
								<xsd:annotation>
									<xsd:documentation>Link to a web address. An Annotation can have an Url.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="value" type="xsd:string" use="required">
							<xsd:annotation>
								<xsd:documentation>The name, term, or text of an annotation. An Annotation must have a value and type.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="type" default="Undefined">
							<xsd:annotation>
								<xsd:documentation>The type of an annotation, e.g. Undefined (default), Ontology, Taxonomy...</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:union>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string">
											<xsd:enumeration value="Undefined"/>
											<xsd:enumeration value="Ontology"/>
											<xsd:enumeration value="Taxonomy"/>
										</xsd:restriction>
									</xsd:simpleType>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string"/>
									</xsd:simpleType>
								</xsd:union>
							</xsd:simpleType>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: Annotation -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Annotations -->
	<!-- ###################################################################### -->
	<!-- 
		AnnotationRef 
		Reference to an Annotation.
		
		AnnotationRef properties in the order to be written: 
		- elementRef
		- CitationRef, EvidenceRef
	-->
	<xsd:element name="AnnotationRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a gpml:Annotation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="gpml:CitationRef" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Reference to a citation for an annotation.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="gpml:EvidenceRef" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Reference to an evidence for an annotation.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="elementRef" type="xsd:IDREF" use="required">
				<xsd:annotation>
					<xsd:documentation>A elementRef indicates a child/parent relationship between objects. The elementRef of the child refers to the elementId of the parent.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- ###################################################################### -->
	<!-- 
		Citations
		Pathway or pathway element CitationRefs refer to Citations. 
		
		Citation properties in the order to be written: 
		- elementId
		- Xref, Url
	-->
	<xsd:element name="Citations">
		<xsd:annotation>
			<xsd:documentation>List of Citations. A citation is a reference to a source of information.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- Citation -->
				<xsd:element name="Citation" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>A literature reference, citation, or work.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:choice>
								<xsd:sequence>
									<xsd:element ref="gpml:Xref">
										<xsd:annotation>
											<xsd:documentation>Reference to a database entry. A Citation must have either an Xref or Url, or both.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element ref="gpml:Url" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Link to a web address. A Citation must have either an Xref or Url, or both.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
								<xsd:element ref="gpml:Url"/>
							</xsd:choice>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: Citation -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Citations -->
	<!-- ###################################################################### -->
	<!-- 
		CitationRef 
		Reference to a Citation.
		
		CitationRef properties in the order to be written: 
		- elementRef
		- AnnotationRef
	-->
	<xsd:element name="CitationRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a gpml:Citation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="gpml:AnnotationRef" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Reference to an annotation for a citation.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="elementRef" type="xsd:IDREF" use="required">
				<xsd:annotation>
					<xsd:documentation>A elementRef indicates a child/parent relationship between objects. The elementRef of the child refers to the elementId of the parent.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: CitationRef -->
	<!-- ###################################################################### -->
	<!-- 
		Evidences
		EvidenceRefs refer to Evidences. Pathway, DataNodes, States, Interactions, and Groups 
		can have Evidence. 
		
		Evidence properties in the order to be written: 
		- elementId, value
		- Xref, Url
	-->
	<xsd:element name="Evidences">
		<xsd:annotation>
			<xsd:documentation>List of Evidence. An evidence provides information on type of scientific evidence.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- Evidence -->
				<xsd:element name="Evidence" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Evidence in biological research.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="gpml:Xref" minOccurs="1" maxOccurs="1">
								<xsd:annotation>
									<xsd:documentation>Reference to a database entry. An Evidence must have an Xref.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element ref="gpml:Url" minOccurs="0" maxOccurs="1">
								<xsd:annotation>
									<xsd:documentation>Link to a web address. An Evidence can have an Url.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="elementId" type="xsd:ID" use="required">
							<xsd:annotation>
								<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="value" type="xsd:string" use="optional">
							<xsd:annotation>
								<xsd:documentation>The name, term, or text of an evidence.</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
				<!-- End: Evidence -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Evidences -->
	<!-- ###################################################################### -->
	<!-- 
		EvidenceRef 
		Reference to an Evidence.
		
		EvidenceRef properties in the order to be written: 
		- elementRef
	-->
	<xsd:element name="EvidenceRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a gpml:Evidence.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="elementRef" type="xsd:IDREF" use="required">
				<xsd:annotation>
					<xsd:documentation>A elementRef indicates a child/parent relationship between objects. The elementRef of the child refers to the elementId of the parent.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: EvidenceRef -->
	<!-- ###################################################################### -->
	<!-- 
		Xref 
		Reference to a database entry.

		Xref properties in the order to be written: 
		- identifier, dataSource		
	-->
	<xsd:element name="Xref">
		<xsd:annotation>
			<xsd:documentation>Reference to a database entry.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="identifier" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation>The identifier of a PublicationXref or Xref object.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="dataSource" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation>Specifies the datasource prefix (see BridgeDb DataSource Ontology at https://github.com/bridgedb/datasources/blob/main/datasources.tsv) for the identifier provided as an Xref.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Xref -->
	<!-- ###################################################################### -->
	<!-- 
		Url 
		Link to a web address and optional description. An Annotation, Citation, and Evidence can have Url.   

		Url properties in the order to be written: 
		- link, description		
	-->
	<xsd:element name="Url">
		<xsd:annotation>
			<xsd:documentation>Link to a web address.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="link" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation>The link to a web address.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="description" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation>A decription for the url.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Xref -->
	<!-- ###################################################################### -->
	<!-- 
		Point
		A location in 2-dimensional space defined by x and y coordinates.
		
		Point properties in the order to be written: 
		- elementId, arrowHead, x, y, elementRef, relX, relY
	-->
	<xsd:element name="Point">
		<xsd:annotation>
			<xsd:documentation>A location in 2-dimensional space defined by x and y coordinates.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="elementId" type="xsd:ID" use="required">
				<xsd:annotation>
					<xsd:documentation>The internal gpml unique identifier for an element.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="arrowHead" default="Undirected">
				<xsd:annotation>
					<xsd:documentation>The glyph at the ends of lines and interactions.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:union>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="Undirected"/>
								<xsd:enumeration value="Directed"/>
								<xsd:enumeration value="Conversion"/>
								<xsd:enumeration value="Inhibition"/>
								<xsd:enumeration value="Catalysis"/>
								<xsd:enumeration value="Stimulation"/>
								<xsd:enumeration value="Binding"/>
								<xsd:enumeration value="Translocation"/>
								<xsd:enumeration value="TranscriptionTranslation"/>
							</xsd:restriction>
						</xsd:simpleType>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string"/>
						</xsd:simpleType>
					</xsd:union>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="x" type="xsd:float" use="required">
				<xsd:annotation>
					<xsd:documentation>The x coordinate of a point.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="y" type="xsd:float" use="required">
				<xsd:annotation>
					<xsd:documentation>The y coordinate of a point.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="elementRef" type="xsd:IDREF" use="optional">
				<xsd:annotation>
					<xsd:documentation>An elementRef indicates a child/parent relationship between objects. The elementRef of the child refers to the elementId of the parent.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="relX" type="xsd:float" use="optional">
				<xsd:annotation>
					<xsd:documentation>When a point is linked to an object, relX and relY are the relative coordinates on the object, where 0,0 is at the center and 1,1 at the bottom-right corner of the object.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="relY" type="xsd:float" use="optional">
				<xsd:annotation>
					<xsd:documentation>When a point is linked to an object, relX and relY are the relative coordinates on the object, where 0,0 is at the center and 1,1 at the bottom-right corner of the object.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Point -->
	<!-- ###################################################################### -->
	<!-- 
		Anchor
		A connection point on a graphical line or an interaction
		
		Anchor properties in the order to be written: 
		- elementId, position, shapeType. 
	-->
	<xsd:element name="Anchor">
		<xsd:annotation>
			<xsd:documentation>A connection point on a graphical line or an interaction, where another graphical line or interaction can be connected.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="elementId" type="xsd:ID" use="required"/>
			<xsd:attribute name="position" type="xsd:float" use="required">
				<xsd:annotation>
					<xsd:documentation>Proportional distance of an anchor along the line it belongs to, between 0 and 1.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="shapeType" default="Square">
				<xsd:annotation>
					<xsd:documentation>The visual representation of an anchor, e.g. Square (default), Circle, None...</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:union>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="Square"/>
								<xsd:enumeration value="Circle"/>
								<xsd:enumeration value="None"/>
							</xsd:restriction>
						</xsd:simpleType>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string"/>
						</xsd:simpleType>
					</xsd:union>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- End: Anchor -->
</xsd:schema>
